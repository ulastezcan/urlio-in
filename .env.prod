# Database Configuration
DB_USER=postgres
DB_PASSWORD=your_strong_db_password_here
DATABASE_URL=postgresql://postgres:your_strong_db_password_here@db:5432/urlioin

# Redis Configuration
REDIS_PASSWORD=your_strong_redis_password_here
REDIS_URL=redis://:your_strong_redis_password_here@redis:6379/0

# JWT Secret Key (Generate a strong random key)
SECRET_KEY=your_jwt_secret_key_here_minimum_32_characters_long

# Domain Configuration
DOMAIN=yourdomain.com
API_URL=https://yourdomain.com/api
ALLOWED_HOSTS=yourdomain.com,www.yourdomain.com

# SSL Configuration
SSL_EMAIL=your-email@yourdomain.com

# Environment
ENVIRONMENT=production
DEBUG=0